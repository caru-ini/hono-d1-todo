{
  "name": "hono-r2-todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "generate": "drizzle-kit generate",
    "migrate:dev": "pnpm dlx wrangler d1 migrations apply sample-todo --local",
    "migrate:prod": "pnpm dlx wrangler d1 migrations apply sample-todo --remote",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.2",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.31.2",
    "drizzle-zod": "^0.5.1",
    "eslint": "^8.57.0",
    "hono": "^4.4.12",
    "lucide-react": "^0.395.0",
    "next": "14.1.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ulid": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.12.0",
    "@cloudflare/vitest-pool-workers": "^0.4.9",
    "@cloudflare/workers-types": "^4.20240620.0",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/runner": "1.5.0",
    "@vitest/snapshot": "1.5.0",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.22.8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-next-on-pages": "^1.12.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vercel": "^34.3.0",
    "vitest": "^1.5.0",
    "wrangler": "^3.63.1"
  }
}
